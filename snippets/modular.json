{
	"modular-child-router": {
		"prefix": "fu-mcr",
		"body": [
			"Router($1, child: (context, args) => $2)"
		]
	},
	"modular-router": {
		"prefix": "fu-mr",
		"body": [
			"Router($1, module: $2),"
		]
	},
	"modular-inital-router": {
		"prefix": "fu-mir",
		"body": [
			"Router(Modular.initialRoute, module: $2),"
		]
	},
	"modular-inital-page-router": {
		"prefix": "fu-mipr",
		"body": [
			"Router(Modular.initialRoute, child: (context, args) => $2)"
		]
	},
	"modular-bind": {
		"prefix": "fu-mbind",
		"body": [
			"Bind((i) => $1($2)),"
		]
	},
	"modular-codegen-inject": {
		"prefix": "fu-modular_codegen_inject",
		"body": [
			"part '${TM_FILENAME_BASE}.g.dart';",
			"",
			"@Injectable($1)"
		]
	},
	"modular-generate-mdule": {
		"prefix": "fu-modular_module",
		"body": [
			"import 'package:flutter_modular/flutter_modular.dart';",
			"",
			"class ${1:${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}} extends ChildModule {",
			"",
			"   @override",
			"   List<Bind> get binds => [];",
			"",
			"   @override",
			"   List<ModularRouter> get routers => [];",
			"",
			"   static Inject get to => Inject<${1:${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}}>.of();",
			"}"
		]
	}
	

}