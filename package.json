{
	"name": "flutter-dart-utils",
	"displayName": "flutter_dart_utils",
	"description": "utilities for working with dart and flutter",
	"publisher": "RodrigoRahman",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.clean-architecture-folders"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.clean-architecture-folders",
				"title": "Clean Arc.: New Feature Backend "
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "extension.clean-architecture-folders",
					"group": "YourGroup@1"
				}
			]
		},
		"snippets": [
			{
			  "language": "dart",
			  "path": "./snippets/dart.json"
			}
		  ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"mkdirp": "^1.0.4"
	}
}
